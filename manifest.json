{
    "name": "YunoHost Wireguard app",
    "id": "ynh_wireguard",
    "packaging_format": 1,
    "description": {
        "en": "A simple, fast and modern VPN",
        "fr": "Un VPN simple, rapide et moderne"
    },
    "version": "0.1",
    "url": "https://wireguard.com",
    "license": " GPL-2.0-only",
    "maintainer": {
        "name": "Dara Ly",
        "email": "contact@libellules.eu",
        "url": "http://www.libellules.eu"
    },
    "requirements": {
        "yunohost": ">= 3.5"
    },
    "multi_instance": true,
    "services": [ ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "ask": {
                    "en": "Choose a domain name for wireguard",
                    "fr": "Choisissez un nom de domaine pour wireguard"
                },
                "example": "example.com"
            },
            {
                "name": "path",
                "type": "path",
                "ask": {
                    "en": "Choose a path for wireguard",
                    "fr": "Choisissez un chemin pour wireguard"
                },
                "example": "/wireguard",
                "default": "/wireguard"
            }
        ]
    }
}
